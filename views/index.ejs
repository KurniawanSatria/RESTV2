<script>
new Vue({
el: '#app',
data: {
sound: new Howl({
src: ['Cry - Cigarettes After Sex (128kbps).mp3'],
volume: 1.0,
preload: true,
onend: function() {
this.isPlaying = false;
}
}),
isPlaying: false
},
methods: {
toggleAudio() {
if (this.isPlaying) {
this.sound.pause();  // Pause the audio if it's currently playing
} else {
this.sound.play();   // Play the audio if it's not playing
}
this.isPlaying = !this.isPlaying; // Toggle the isPlaying state
}
}
});

const modeToggle = $('#modeToggle');
const modeIcon = $('#modeIcon');
let currentMode = body.attr('data-theme-version') || 'light'; 
if (currentMode === 'dark') {
modeIcon.removeClass('fa-sun').addClass('fa-moon'); 
} else {
modeIcon.removeClass('fa-moon').addClass('fa-sun');  
}


modeToggle.on('click', () => {
if (currentMode === 'light') {
currentMode = 'dark'; 
body.attr('data-theme-version', 'dark');
modeIcon.removeClass('fa-sun').addClass('fa-moon'); 
} else {
currentMode = 'light'; // Update mode
body.attr('data-theme-version', 'light');
modeIcon.removeClass('fa-moon').addClass('fa-sun');  
}
dlabSettingsOptions.version = currentMode;
new dlabSettings(dlabSettingsOptions); // Update pengaturan dengan mode yang baru
let isOnline = true, intervalId, timer = 10, wasOffline = false;

const checkConnection = async () => {
try {
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
isOnline = response.status >= 200 && response.status < 300;
} catch (error) {
isOnline = false; // Jika terjadi kesalahan, dianggap offline
}
timer = 10;
clearInterval(intervalId);
handlePopup(isOnline);
};
const handlePopup = (status) => {
if (status) {
if (wasOffline) {
Swal.fire({
icon: 'success',
title: 'Restored Connection',
text: 'Your device is now successfully connected to the internet.',
timer: 2000,
showConfirmButton: false,
toast: true,
position: 'top-end'
});
}
wasOffline = false; 
} else {
wasOffline = true; 
Swal.fire({
icon: 'error',
title: 'Lost Connection',
html: `Your network is unavailable. We will attempt to reconnect you in <b>${timer}</b> seconds.`,
showConfirmButton: false,
allowOutsideClick: false,
timerProgressBar: true,
didOpen: () => {
intervalId = setInterval(() => {
timer--;
if (timer === 0) {
Swal.close(); 
checkConnection(); 
}
const content = Swal.getHtmlContainer();
if (content) {
const b = content.querySelector('b');
if (b) b.textContent = timer;
}
}, 1000);
},
willClose: () => {
clearInterval(intervalId); 
}
});
}
};

// Periksa status koneksi setiap 3 detik jika online
setInterval(() => isOnline && checkConnection(), 3000);

});

</script>
